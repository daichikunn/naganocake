<h2>ジャンル一覧・追加</h2>
<%= form_with model: @genre,url: admin_genres_path, method: :post do |f| %>

  <div class="field">
    <%= f.label "ジャンル名" %><br />
    <%= f.text_field :name, placeholder: "ジャンル名" %>
  </div>
  <%= f.submit "新気登録"%>
<% end %>

<% @genres.each do |genre|%>

  <%= genre.name %>

<% end %>